<mvc:View
        controllerName="sts.drawable.controller.Drawing"
        displayBlock="true"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:sts="sts.drawable.controls.drawing"
>
    <Page>
        <sts:Drawing id="drawing"
                     newOperation="onNewOperation"
                     objectDeleted="onObjectDeleted"
                     selectionChange="onSelectionChange"
                     objects="{appModel>/drawing}"
        />
    </Page>
</mvc:View>
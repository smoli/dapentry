{
  "ignore": [
    "**/*.d.ts"
  ],
  "presets": [
    [
      "transform-ui5",
      {
        "modulesMap": {}
      }
    ],
    [
      "@babel/typescript",
      {
        "module": "commonjs"
      }
    ]
  ],
  "plugins": [
    "babel-plugin-transform-async-to-promises",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ]
}
{
  "name": "gfxbcts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register --recursive test/**/*.ts",
    "testArg": "mocha -r ts-node/register ",
    "coverage": "nyc npm run test",
    "coverageArg": "nyc npm run testArg",
    "build": "npm-run-all build:ts build:ui5",
    "build:opt": "npm-run-all build:ts build:ui5:opt",
    "build:ts": "babel src --out-dir build --source-maps true --extensions \".ts,.js\" --copy-files",
    "build:ui5": "ui5 build --clean-dest",
    "build:ui5:opt": "ui5 build self-contained --clean-dest --all",
    "start": "npm-run-all --parallel watch:controls watch:ts start:ui5",
    "watch:ts": "babel src --out-dir build --source-maps true --extensions \".ts,.js\" --copy-files --watch",
    "start:ui5": "ui5 serve --port 8080 -o index.html",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "watch:controls": "npx @ui5/ts-interface-generator --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@openui5/sap.f": "^1.97.0",
    "@openui5/sap.m": "^1.97.0",
    "@openui5/sap.ui.core": "^1.97.0",
    "@openui5/themelib_sap_belize": "^1.97.0",
    "@types/chai": "^4.3.0",
    "@types/jquery": "^3.5.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.5",
    "@types/openui5": "^1.97.0",
    "@types/qunit": "^2.5.4",
    "@ui5/cli": "^2.14.2",
    "@ui5/ts-interface-generator": "^0.2.3",
    "babel-plugin-transform-async-to-promises": "^0.8.17",
    "babel-preset-transform-ui5": "^7.0.5",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "pegjs": "^0.10.0",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "ui5-middleware-livereload": "^0.5.8"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.16.5"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "pegjs"
    ]
  }
